<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"></plugin>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/zhiyuan?serverTimezone=GMT%2B8&amp;nullCatalogMeansCurrent=true"
                        userId="root"
                        password="root">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.sanelee.zhiyuan.Model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.sanelee.zhiyuan.Mapper"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table schema="zhiyuan" tableName="user" domainObjectName="User" ></table>
        <table schema="zhiyuan" tableName="school" domainObjectName="School" ></table>
        <table schema="zhiyuan" tableName="profession" domainObjectName="Profession" ></table>
        <table schema="zhiyuan" tableName="gaokao" domainObjectName="GaoKao" ></table>
        <table schema="zhiyuan" tableName="schoolscore" domainObjectName="SchoolScore" ></table>
        <table schema="zhiyuan" tableName="question" domainObjectName="Question" ></table>

    </context>
</generatorConfiguration>